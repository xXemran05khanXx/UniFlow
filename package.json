{
  "name": "uniflow",
  "version": "1.0.0",
  "description": "UniFlow Timetable Management System",
  "main": "index.js",
  "scripts": {
    "install-deps": "cd backend && npm install && cd ../frontend && npm install",
    "build": "npm run build:client && npm run build:server",
    "start": "npm run start:server",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "deploy": "npm run install-deps && npm run build",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "cd client && vite",
    "dev:server": "cd server && tsx watch index.ts",
    "build:client": "cd client && vite build",
    "build:server": "cd server && tsc",
    "start:server": "cd server && node index.js",
    "type-check": "tsc --noEmit",
    "setup": "node setup-project.js"
  },
  "keywords": [
    "timetable",
    "university",
    "management",
    "scheduling"
  ],
  "author": "UniFlow Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
