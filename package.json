{
  "name": "uniflow",
  "version": "1.0.0",
  "description": "UniFlow Timetable Management System",
  "main": "index.js",
  "scripts": {
    "install-deps": "cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "deploy": "npm run install-deps && npm run build"
  },
  "keywords": [
    "timetable",
    "university",
    "management",
    "scheduling"
  ],
  "author": "UniFlow Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "axios": "^1.11.0",
    "lucide-react": "^0.536.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.7.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.3"
  }
}
